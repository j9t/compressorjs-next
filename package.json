{
  "name": "compressorjs-next",
  "version": "2.0.0",
  "description": "JavaScript image compressor.",
  "main": "dist/compressor.common.js",
  "module": "dist/compressor.esm.js",
  "browser": "dist/compressor.js",
  "types": "types/index.d.ts",
  "files": [
    "src",
    "dist",
    "types"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "del-cli dist",
    "compress": "terser dist/compressor.js -o dist/compressor.min.js -c -m --comments /^!/",
    "dev": "rollup -c -m -w",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint docs/**/*.css --fix",
    "lint:js": "eslint src test *.js --fix",
    "prepare": "husky",
    "release": "npm run clean && npm run lint && npm run build && npm run compress && npm test",
    "start": "npm run dev",
    "test": "cross-env NODE_ENV=test karma start test/karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j9t/compressorjs-next.git"
  },
  "keywords": [
    "image",
    "compress",
    "compressor",
    "compressor.js",
    "image-compressor",
    "lossy-compression",
    "javascript",
    "front-end",
    "web"
  ],
  "author": {
    "name": "Jens Oliver Meiert",
    "url": "https://meiert.com/"
  },
  "contributors": [
    {
      "name": "Chen Fengyuan",
      "url": "https://chenfengyuan.com/"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/j9t/compressorjs-next/issues"
  },
  "homepage": "https://github.com/j9t/compressorjs-next",
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/plugin-transform-object-assign": "^7.27.1",
    "@babel/preset-env": "^7.28.6",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "babel-plugin-istanbul": "^7.0.1",
    "chai": "^6.2.2",
    "cross-env": "^10.1.0",
    "del-cli": "^7.0.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "karma": "^6.4.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-rollup-preprocessor": "^7.0.8",
    "lint-staged": "^16.2.7",
    "mocha": "^11.7.5",
    "puppeteer": "^24.36.0",
    "rollup": "^4.56.0",
    "stylelint": "^17.0.0",
    "stylelint-config-standard": "^40.0.0",
    "stylelint-order": "^7.0.1",
    "terser": "^5.37.0"
  }
}
